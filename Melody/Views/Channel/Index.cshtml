@model IEnumerable<Melody.Models.Channel>

@{
    ViewData["Title"] = "Index";
}
<style>
    .Channel {
        width: auto;
        height: auto;
        background-color: #333;
        border-radius:25px;
        padding:15px;
    }
    .Channel h3{
        margin-top:0px;
    }
</style>
<div class="container">
    @await Html.PartialAsync("_leftDiv")
    <div class="div-right">
        @await Html.PartialAsync("_rightDiv")
        <div class="Channel">
            <h3>Channel</h3>
        </div>
    </div>
</div>